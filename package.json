{
    "name": "running",
    "version": "0.0.1",
    "private": true,
    "description": "Lightweight fitness tracking app for runners",
    "main": "src/backend/index.ts",
    "scripts": {
        "build": "npm run build:frontend && npm run build:backend",
        "build:backend": "tsc",
        "build:frontend": "parcel build --no-minify -d dist/frontend src/frontend/index.html",
        "start": "node dist/backend/index.js",
        "dev:backend": "nodemon -e ts --exec \"ts-node src/backend/index.ts\"",
        "dev:frontend": "parcel watch -d dist/frontend src/frontend/index.html",
        "test:backend": "jest --rootDir test/backend",
        "tdd:backend": "jest --rootDir test/backend --watchAll --all"
    },
    "author": "Adam Neumann",
    "license": "ISC",
    "dependencies": {
        "express": "^4.17.1",
        "fit-decoder": "^1.0.5",
        "luxon": "^1.28.0",
        "zebras": "^0.0.19"
    },
    "devDependencies": {
        "@nivo/bar": "^0.73.1",
        "@nivo/core": "^0.73.0",
        "@nivo/line": "^0.73.0",
        "@types/express": "^4.17.1",
        "@types/jest": "^26.0.24",
        "@types/leaflet": "^1.7.4",
        "@types/luxon": "^1.27.1",
        "@types/parcel-env": "^0.0.1",
        "@types/react": "^17.0.2",
        "@types/react-dom": "^17.0.2",
        "@types/react-router-dom": "^5.1.8",
        "axios": "^0.21.1",
        "jest": "^26.0.24",
        "leaflet": "^1.7.1",
        "milligram": "^1.4.1",
        "nodemon": "^2.0.4",
        "parcel-bundler": "^1.12.5",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-leaflet": "^3.2.0",
        "react-router-dom": "^5.1.8",
        "ts-jest": "^26.0.24",
        "ts-node": "^10.1.0",
        "typescript": "^4.3.5"
    }
}
